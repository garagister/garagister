<%= simple_form_for(@garagiste) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :ville %>
    <%= f.input :code_postal %>
    <%= f.input :categorie %>
    <%= f.input :taux_mo %>
    <%= f.input :taux_piece %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
